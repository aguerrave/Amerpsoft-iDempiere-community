	<subDataset name="InvoiceCxPFormatWH" uuid="77e92c23-dcd5-419c-82f2-3b7ec3058260">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="idempiereTAM2.0dev"/>
		<queryString>
			<![CDATA[SELECT
adempiere.c_taxcategory.withholdingformat,
adempiere.c_taxcategory.name as taxname,
coalesce(adempiere.lco_invoicewithholding.percent,0) as percent,
coalesce(adempiere.lco_invoicewithholding.taxamt,0) as taxamtwh,
coalesce(adempiere.lco_invoicewithholding.taxbaseamt,0) as taxbaseamt,
sum(adempiere.lco_invoicewithholding.taxamt) OVER (ORDER BY adempiere.c_taxcategory.withholdingformat) as total_wh
FROM
adempiere.c_invoice
JOIN adempiere.c_invoiceline on adempiere.c_invoice.c_invoice_id = adempiere.c_invoiceline.c_invoice_id
JOIN adempiere.lco_invoicewithholding on adempiere.c_invoice.c_invoice_id = adempiere.lco_invoicewithholding.c_invoice_id
JOIN adempiere.c_tax on adempiere.lco_invoicewithholding.c_tax_id = adempiere.c_tax.c_tax_id
LEFT JOIN adempiere.c_taxcategory ON adempiere.c_tax.c_taxcategory_id = adempiere.c_taxcategory.c_taxcategory_id
WHERE
adempiere.c_invoice.c_invoice_id=1000001
GROUP BY
adempiere.c_taxcategory.withholdingformat,
adempiere.c_taxcategory.name,
adempiere.lco_invoicewithholding.percent,
adempiere.lco_invoicewithholding.taxamt,
adempiere.lco_invoicewithholding.taxbaseamt
ORDER BY
adempiere.c_taxcategory.name ASC]]>
		</queryString>
		<field name="taxname" class="java.lang.String"/>
		<field name="percent" class="java.math.BigDecimal"/>
		<field name="taxamtwh" class="java.math.BigDecimal"/>
		<field name="taxbaseamt" class="java.math.BigDecimal"/>
		<group name="taxname">
			<groupExpression><![CDATA[$F{taxname}]]></groupExpression>
		</group>
